<configuration>
    <system.web>
      <compilation debug="true" targetFramework="4.0" />
		<httpRuntime maxRequestLength="104857600"/>
    </system.web>
	<system.webServer>
		<security>
			<requestFiltering>
				<requestLimits maxAllowedContentLength="104857600"/>
			</requestFiltering>
		</security>
	</system.webServer>
</configuration>
